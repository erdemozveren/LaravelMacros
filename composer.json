{
    "name": "erdemozveren/laravelmacros",
    "description": "Laravel helper macros for your project",
    "license": "MIT",
    "authors": [
        {
            "name": "Erdem Ã–zveren",
            "email": "erdemozveren35@gmail.com",
            "homepage": "http://erdemozveren.com"
        }
    ],
    "homepage": "https://github.com/erdemozveren/laravelmacros",
    "keywords": ["Laravel", "macro","formbuilder"],
    "require": {
        "illuminate/support": "~5",
        "php": "^7.1.3",
        "laravelcollective/html": "^5.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "erdemozveren\\laravelmacros\\": "src/"
        },
        "files":[
            "src/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "erdemozveren\\laravelmacros\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
        },
        "laravel": {
            "providers": [
                "erdemozveren\\laravelmacros\\laravelmacrosServiceProvider"
            ],
            "aliases": {
                "laravelmacros": "erdemozveren\\laravelmacros\\Facades\\laravelmacros",
                "Form": "Collective\\Html\\FormFacade",
                "Html": "Collective\\Html\\HtmlFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
